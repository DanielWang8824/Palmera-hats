{
    "extends": "solhint:all",
    "rules": {
      "compiler-version": ["warning", ">=0.8.10 <0.9.0"],
      "func-visibility": ["error", { "ignoreConstructors": true }],
      "comprehensive-interface": ["off"],
      "const-name-snakecase": ["error"],
      "ordering": ["error"],
      "private-vars-leading-underscore": ["warning"],
      "max-line-length" : ["warning"]
    }
}